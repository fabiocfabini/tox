func main() {
    WIDTH : int = 100
    HEIGHT: int = 100

    limit: int = 70

    for(g1: int = -WIDTH; g1 <= WIDTH; g1 = g1 + 1) {
        for(g2: int = HEIGHT; g2 >= -HEIGHT; g2 = g2 - 1) {
            if(0 <= g1*g2 && g1*g2 <= limit ) {
                print("#")
            } else {
                print(" ")
            }
        }
        print("\n")
    }
}