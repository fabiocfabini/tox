munus main() {
    // Read from user
    x: integer
    x = legerei("Type a number: ")

    par x {
        0 -> {
            imprimo("x is zero\n")
        }

        1 -> {
            imprimo("x is one\n")
        }

        2 -> {
            imprimo("x is two\n")
        }

        defectus -> {
            imprimo("x is another number\n")
        }
    }
}