func test_pi() -> float {
    N: int = 1000
    pi: float = 0.0

    for (i: int = 0; i < N; i = i + 1) {
        pi = pi + (1.0 - 2.0 * (float) (i % 2)) / (2.0 * (float) i + 1.0)
    }

    return 4.0 * pi
}

func test_e() -> float {
    N: int = 100
    fact: float = 1.0
    e: float = 1.0

    for (i: int = 1; i < N; i = i + 1) {
        fact = fact * (float) i
        e = e + 1.0 / fact
    }

    return e
}


